<Page x:Class="PassKeeper_WPF.Views.Pages.RegisterPasswordPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
xmlns:local="clr-namespace:PassKeeper_WPF.Views.Pages"
xmlns:md ="http://materialdesigninxaml.net/winfx/xaml/themes"
mc:Ignorable="d" 
d:DesignHeight="700" d:DesignWidth="375"
Title="PassKeeper">

    <Grid>

        <StackPanel>
            <Image Width="212" Height="60" Margin="0 110 0 0"  HorizontalAlignment="Center" VerticalAlignment="Center"  Source="\Resources\Images\passkeeper main logo.png"/>
            <TextBox Margin="0 70" Text="{Binding newU.Password}"/>
            <TextBox Name="ConfirmPassInput"  Text=" confirm password"/>
            <Button Margin="0 10" Content="Confirm " Click="Button_Click"/>

        </StackPanel>     
        
        
        
        
    </Grid>
</Page>
